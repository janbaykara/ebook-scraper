{
  "name": "eBook Scraper",
  "version": "1.1.1",
  "description": "Save eBook pages as a PDF",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/eventPage.js",
    "persistent": true
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "512": "icon-512.png"
    }
  },
  "icons": {
    "512": "icon-512.png",
    "128": "icon-128.png",
    "48": "icon-48.png",
    "16": "icon-16.png"
  },
  "permissions": [
    "tabs",
    "declarativeContent",
    "storage",
    "unlimitedStorage",
    "downloads",
    "webRequest"
  ],
  "host_permissions": []
}